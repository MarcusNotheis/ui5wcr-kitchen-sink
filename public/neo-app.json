{
  "authenticationMethod": "saml",
  "welcomeFile": "/index.html",
  "sendWelcomeFileRedirect": true,
  "securityConstraints": [
    {
      "permission": "p_consumer",
      "description": "Consumer of the Application",
      "protectedPaths": ["/"]
    },
    {
      "permission": "AccessApplicationDescriptor",
      "description": "AccessApplicationDescriptor",
      "protectedPaths": ["/neo-app.json"]
    }
  ],
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "*html"
    },
    {
      "directive": "public",
      "path": "static/",
      "maxAge": 31536000
    },
    {
      "directive": "public",
      "maxAge": 3600
    }
  ]
}
